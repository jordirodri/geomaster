<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.sillynamedvars.geomaster.pro"
      android:versionCode="3"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="7" />

	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<uses-library android:name="com.google.android.maps" />

		<activity 
		android:name=".Menu1GameActivity" 
		android:label="@string/app_name"
		android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
		android:screenOrientation="nosensor" android:configChanges="keyboardHidden|orientation">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		
		<activity android:name="GeoMaster"
		 android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
		  android:screenOrientation="nosensor" android:configChanges="keyboardHidden|orientation">
		</activity>
		<activity android:name="Menu2GameActivity"
		android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
		android:screenOrientation="nosensor" android:configChanges="keyboardHidden|orientation"></activity>

		<activity android:name="Menu3GameActivity"
		android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
		android:screenOrientation="nosensor" android:configChanges="keyboardHidden|orientation"></activity>

		<activity android:name="InstructionsActivity"
		android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
		android:screenOrientation="nosensor" android:configChanges="keyboardHidden|orientation"></activity>
		
		<activity android:name="SettingsActivity"
		android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
		android:screenOrientation="nosensor" android:configChanges="keyboardHidden|orientation"></activity>
		
		<activity android:name="ScoresActivity"
		android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
		android:screenOrientation="nosensor" android:configChanges="keyboardHidden|orientation"></activity>

		<activity android:name="com.google.ads.AdActivity"
              android:configChanges="keyboard|keyboardHidden|orientation"/>

	</application>
	
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
 
    
</manifest>