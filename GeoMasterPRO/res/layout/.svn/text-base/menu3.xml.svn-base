<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/menu1Layout" 
	xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads"	
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    style="@style/MenuScreen3">
    
     
 <ScrollView
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  android:layout_above="@+id/adView"
  android:layout_alignParentTop="true">
                          
<LinearLayout android:orientation="vertical" 
	android:id="@+id/linearLayout1" 
	android:layout_width="fill_parent" 
	android:layout_gravity="top" 
	android:padding="10dp" 
	android:layout_height="fill_parent">

    <ImageView 
    android:src="@drawable/geomastertitlepro" 
    android:id="@+id/imageView1"    
    android:layout_width="fill_parent" 
    android:layout_gravity="center" android:layout_height="50dp">
    </ImageView>
        
    <Button style="@style/MenuButton" 
    		android:layout_height="50dp"     		
    		android:layout_width="200dp"     		
    		android:id="@+id/buttonEasyPlay" 
    		android:text="@string/button_play_easy" 
    		android:layout_gravity="center"  
    		android:layout_marginTop="20dp"
    		android:layout_marginBottom="10dp"></Button>
    		
    <Button style="@style/MenuButton" 
    		android:layout_height="50dp"     		
    		android:layout_width="200dp"     		
    		android:id="@+id/buttonNormalPlay" 
    		android:text="@string/button_play_normal" 
    		android:layout_gravity="center"  
    		android:layout_margin="10dp"></Button>
    		
    <!-- PRO VERSION -->		
    <Button style="@style/MenuButton" 
    		android:layout_height="50dp"     		
    		android:layout_width="200dp"     		
    		android:id="@+id/buttonHardPlay" 
    		android:text="@string/button_play_hard" 
    		android:layout_gravity="center"  
    		android:layout_margin="10dp"></Button>
    		
    <!-- FREE VERSION -->	
    <!-- 		
    <Button style="@style/MenuButtonLock" 
    		android:layout_height="50dp"     		
    		android:layout_width="200dp"   
    		
        	android:paddingLeft="20dp"	
        	android:paddingRight="40dp"	  		
    		android:id="@+id/buttonHardPlay" 
    		android:text="@string/button_play_hard" 
    		android:layout_gravity="center"  
    		android:layout_margin="10dp"></Button>
    		
    		-->		
    		 
    				    
</LinearLayout>
</ScrollView>

  <!-- com.google.ads.AdView android:id="@+id/adView"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         ads:adUnitId="@string/admob_unit_id"
                         ads:adSize="BANNER"
                         ads:loadAdOnCreate="true"
                         android:layout_alignParentBottom="true"/-->
</RelativeLayout>
